# editorconfig.org
root = true

[*]
charset = utf-8
indent_style = space
indent_size = 2
insert_final_newline = true
trim_trailing_whitespace = true

[*.{js,json}]
indent_style = space
indent_size = 2

[.md]
insert_final_newline = false
trim_trailing_whitespace = false

[*.{java,xml}]
indent_size = 4
continuation_indent_size = 8